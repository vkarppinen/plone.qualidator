<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="plone.qualidator">

  <plone:behavior
      title="Content qualidation"
      description="Adds content qualidation behaviour to dexterity types."
      provides=".qualidator.IQualidatable"
      />

  <subscriber
      for=".qualidator.IQualidatable zope.lifecycleevent.IObjectModifiedEvent"
      handler=".qualidator.handle_redirect"
      />


</configure>
